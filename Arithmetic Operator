Write a C program to do an Arithmetic Operators.
#include<iostream>
#include<cmath>
using namespace std;

int main()
{
    int a;
    int b;
    cin >> a >> b;
    cout << a << "+" << b << "=" << (a+b) << endl;
    cout << a << "-" << b << "=" << (a-b) << endl;
    cout << a << "*" << b << "=" << (a*b) << endl;
    cout << a << "/" << b << "=" << (a/b) << endl;
    cout << a << "%" << b << "=" << (a%b) << endl;
    return 0;
}
